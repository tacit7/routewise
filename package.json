{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:no-msw": "NODE_ENV=development MSW_DISABLED=true tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "node test/performance/run-performance-tests.js",
    "test:performance:lighthouse": "node test/performance/run-performance-tests.js lighthouse",
    "test:performance:load": "node test/performance/run-performance-tests.js load",
    "test:performance:bundle": "node test/performance/run-performance-tests.js bundle",
    "test:a11y": "playwright test test/accessibility/a11y.spec.ts",
    "test:a11y:report": "playwright test test/accessibility/a11y.spec.ts --reporter=html",
    "test:all": "npm run test && npm run test:e2e && npm run test:performance && npm run test:a11y",
    "msw:init": "msw init client/public --save"
  },
